<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info" strict="true" monitorInterval="5">
    <Properties>
        <Property name="logName">EmbeddedWebserverTest</Property>
        <Property name="logPath">logs</Property>
    </Properties>
    <Appenders>
        <Appender type="Console" name="consoleOut" target="SYSTEM_OUT">
            <Layout type="PatternLayout" pattern="%d %-5p [%t] %c: %m%n" />
        </Appender>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="consoleOut" />
        </Root>
    </Loggers>
</Configuration>
